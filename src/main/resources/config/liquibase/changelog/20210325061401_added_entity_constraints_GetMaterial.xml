<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity GetMaterial.
    -->
    <changeSet id="20210325061401-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="material_id"
                                 baseTableName="get_material"
                                 constraintName="fk_get_material_material_id"
                                 referencedColumnNames="id"
                                 referencedTableName="material"/>

        <addForeignKeyConstraint baseColumnNames="receptionist_id"
                                 baseTableName="get_material"
                                 constraintName="fk_get_material_receptionist_id"
                                 referencedColumnNames="id"
                                 referencedTableName="receptionist"/>

    </changeSet>
</databaseChangeLog>
